{
    "description": "各テストケース（normal, value_errorなど）ごとに、関数の引数名や型に基づいてテスト値を生成するルールを定義します。テストケースは目的ごとに複数定義できます。",
    "normal": {
        "match_count": 2,
        "argument_rules": [
            {"type": "numeric", "argument_match": ["num", ".+count", ".+size", ".+length"],
            "value_list": [1, 2, 3, 4, 5]},
            {"type": "string", "argument_match": ["name", "str"],
            "value_list": ["name1", "test_name", "valid", "path"]},
            {"type": "string", "argument_match": ["text"],
            "value_list": ["text1", "text2", "text3", "text4", "text5"]},
            {"type": "string", "argument_match": ["text"],
            "value_list": [ "一行\n複数行"]},
            {"type": "string", "argument_match": ["text"],
            "value_list": ["<xml>data</xml>"]},
            {"type": "string", "argument_match": [".*dll_path"],
            "value_list": ["../../../Tools/diff.dll", "./mydll.dll", "./valid.dll"]},
            {"type": "string", "argument_match": [".*cs_file_path"],
            "value_list": ["./Sources/Test/valid_code.cs", "/tmp/valid_code.cs"]},
            {"type": "string", "argument_match": [".*text_file_path"],
            "value_list": ["./Sources/Test/valid_text.txt", "C:\\data\\valid.txt"]},
            {"type": "int", "argument_match": ["id"], "value_list": [10, 0, -1, 999]},
            {"type": "bool", "argument_match": ["flag", "is_"], "value_list": [true, false]},
            {"type": "DateTime", "argument_match": ["date", "time"],
            "value_list": ["2023-10-27T10:00:00", "1970-01-01T00:00:00", "2024-12-31T23:59:59"]},
            {"type": "string[]", "argument_match": ["array"],
            "value_list": [["a"], ["test", "sample"], ["item1", "item2"]]},
            {"type": "Dictionary<string,string>", "argument_match": ["settings"],
            "value_list": [{"key1": "value1"}, {"settingA": "1", "settingB": "true"}, {"path": "/opt/data"}]},
            {"type": "Dictionary`2", "argument_match": ["array"], "value_list": [["a"], ["test", "sample"]]},
            {"type": "string", "argument_match": ["helpFolder"], "value_list": ["./help", "/opt/help", "/usr/share/doc"]},
            {"type": "string[]", "argument_match": ["configFolders"],
            "value_list": [["./config"], ["/etc/config", "/usr/local/config"], ["C:\\config"]]},
            {"type": "Object", "argument_match": ["settings"], "value_list": [{}, {"a": 1}, {"valid": true}]},
            {"type": "Object", "argument_match": ["helpFolder"], "value_list": ["valid_help_string", {"path": "./help"}]},
            {"type": "Object", "argument_match": ["configFolders"], "value_list": [["valid_folder"], {"paths": ["/etc/config"]}]},
            {"type": "Object", "function_match": ".*Equ.*", "argument_match": ["obj"], "value_list": [{"key1": "value1"}, {"key2": "value2"}, {"key3": "value3"}, {"key4": "value4"}]},
            {"type": "Object", "argument_match": ["obj"], "value_list": [123, "test", true, {"key": "value"}]},
            {"type": "string", "argument_match": ["build_method"], "value_list": ["csc", "msbuild"]},
            {"type": "string", "argument_match": ["cs_compiler_path"],
            "value_list": ["C:\\Windows\\Microsoft.NET\\Framework\\v4.0.30319\\csc.exe", "C:\\Program Files (x86)\\Microsoft Visual Studio\\2019\\Community\\MSBuild\\Current\\Bin\\MSBuild.exe"]},
            {"type": "string[]", "argument_match": ["references"],
            "value_list": [["System.dll"], ["System.Core.dll", "System.Data.dll"]]},
            {"type": "string", "argument_match": ["file_path"], "value_list": ["./dummy.dll", "./image.png", "./config.txt"]},
            {"type": "Assembly", "argument_match": ["assembly"], "value_list": ["valid_assembly_object_1", "valid_assembly_object_2"]},
            {"type": "string", "argument_match": ["class_name"], "value_list": ["ValidClassA", "Another.ValidClassB"]}
        ]
    },
    "value_error": {
        "match_count": 2,
        "argument_rules": [
            {"type": "numeric", "argument_match": ["num", ".+count", ".+size", ".+length"],
            "value_list": [null, "abc", true]},
            {"type": "string", "argument_match": ["name", "str", "text", ".*_path"],
            "value_list": [1, true, null]},
            {"type": "int", "argument_match": ["id"], "value_list": [null, "abc", true]},
            {"type": "bool", "argument_match": ["flag", "is_"], "value_list": [null, 0, 1]},
            {"type": "DateTime", "argument_match": ["date", "time"], "value_list": ["invalid date", 123, null]},
            {"type": "string[]", "argument_match": ["array"], "value_list": ["not an array", 1, null]},
            {"type": "Dictionary<string,string>", "argument_match": ["settings"], "value_list": ["not a dict", 1, null]},
            {"type": "Dictionary`2", "argument_match": ["array"], "value_list": [["a"], ["test", "sample"]]},
            {"type": "string", "argument_match": ["helpFolder"], "value_list": [123, null]},
            {"type": "string[]", "argument_match": ["configFolders"], "value_list": ["not an array", null]},
            {"type": "Object", "argument_match": ["settings"], "value_list": [123, "string"]},
            {"type": "Object", "argument_match": ["helpFolder"], "value_list": [1]},
            {"type": "Object", "argument_match": ["configFolders"], "value_list": ["string"]},
            {"type": "Object", "argument_match": ["obj"], "value_list": [["a"], {"b": 2}]},
            {"type": "string", "argument_match": ["build_method"], "value_list": ["invalid_method", 123, null]},
            {"type": "string", "argument_match": ["cs_compiler_path"], "value_list": [123, true]},
            {"type": "string[]", "argument_match": ["references"], "value_list": ["not an array", 123]},
            {"type": "string", "argument_match": ["file_path"], "value_list": [null, 123, true]},
            {"type": "Assembly", "argument_match": ["assembly"], "value_list": [null, 123, "string"]},
            {"type": "string", "argument_match": ["class_name"], "value_list": [null, 123, true]}
        ]
    }
}