{
    "version": 1,
    "disable_existing_loggers": false,
    "handlers": {
        "file": {
            "class": "logging.FileHandler",
            "filename": "../Log/log.json",
            "formatter": "json"
        }
    },
    "formatters": {
        "json": {
            "()": "pythonjsonlogger.jsonlogger.JsonFormatter",
            "format": "%(asctime)s %(levelname)s %(message)s",
            "datefmt": "%Y-%m-%d %H:%M:%S",
            "json_default": 