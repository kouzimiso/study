{
    "version": 1,
    "disable_existing_loggers": false,
    "handlers": {
        "file": {
            "class": "logging.FileHandler",
            "filename": "../Log/log2.json",
            "formatter": "json"
        },
        "console": {
            "class": "logging.StreamHandler",
            "formatter": "json",
            "level": "INFO"
        }
    },
    "formatters": {
        "json": {
            "format": "%(message)s",
            "datefmt": "%Y/%m/%d %H:%M:%S",
            "class": "pythonjsonlogger.jsonlogger.JsonFormatter",
	}
    }
}
 